{
  "name": "vite-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf server && rm -rf dist && rm -rf node_modules",
    "dev": "concurrently \"npm run dev:ui\" \"vite\" \"npm run dev:listen\" \"npm run dev:server\" ",
    "dev:ui": "vite",
    "dev:listen": "onchange 'src/**/*.tsx' -- npm run build",
    "dev:server": "vite build && tsx watch ./src/server.tsx",
    "build": "concurrently  \"npm run build:ui\" ",
    "build:ui": "vite build --emptyOutDir",
    "start": "tsx src/server.tsx"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "helmet-csp": "^3.4.0",
    "morgan": "^1.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "vite": "^4.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "concurrently": "^7.6.0",
    "onchange": "^7.1.0",
    "tsx": "^3.12.6",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
